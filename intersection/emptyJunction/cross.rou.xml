<routes>
        <vType id="bus" vClass="bus"/>
        <vType id="bicycle" vClass="bicycle"/>

        <!--Only approach the intersection-->
        <route id="W-to-intersection" edges="1i" />
        <route id="SW-to-intersection" edges="2i" />
        <route id="S-to-intersection" edges="3i" />
        <route id="E-to-intersection" edges="4i" />
        <route id="NE-to-intersection" edges="5i" />
        
        <!--Pass (approach and leave) the intersection-->
        <route id="W-to-E" edges="1i 4o" />
        <route id="SW-to-NE" edges="2i 5o" />
        <route id="S-to-NE" edges="3i 5o" />
        <route id="E-to-W" edges="4i 1o" />
        <route id="NE-to-SW" edges="5i 2o" />

        <!--Only leave the intersection-->
        <route id="intersection-to-W" edges="1o" />
        <route id="intersection-to-SW" edges="2o" />
        <route id="intersection-to-E" edges="4o" />
        <route id="intersection-to-NE" edges="5o" />
</routes>
